{
  "name": "caliper-samples",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperledger/caliper.git"
  },
  "scripts": {
    "pretest": "npm run licchk",
    "licchk": "license-check-and-add",
    "test": "exit 0"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0"
  },
  "dependencies": {
    "@hyperledger/caliper-cli": "^0.1.0",
    "fabric-ca-client": "^1.4.1",
    "fabric-client": "^1.4.1",
    "fabric-network": "^1.4.1",
    "fabric-protos": "^2.0.0-snapshot.1",
    "grpc": "^1.24.9"
  },
  "devDependencies": {
    "license-check-and-add": "2.3.6"
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "../../LICENSE.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      "node_modules",
      ".nyc_output",
      "coverage",
      "log",
      "benchmark/smallbank/.Rhistory",
      "network/burrow/simple/chain/keys/names",
      "network/sawtooth/simplenetwork/Dockerfile",
      "network/fabric-v1.0/config/.gitignore",
      "network/fabric-v1.1/config/.gitignore",
      "network/fabric-v1.2/config/.gitignore",
      "network/fabric-v1.3/config/.gitignore",
      "network/fabric-v1.4/config/.gitignore",
      "network/fabric-v1.4/kafka/config/.gitignore",
      "network/fabric-v1.4.1/config/.gitignore",
      "network/fabric-v1.4.1/raft/config/.gitignore",
      "src/contract/sawtooth/simple/simple_python/simple-tp-python",
      "src/contract/sawtooth/simple/simple_python/packaging/systemd/sawtooth-simple-tp-python",
      "src/contract/sawtooth/docker/sawtooth-int-simple-tp-python"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".yml",
      ".log",
      ".acl",
      ".md",
      ".toml",
      ".sample",
      ".service",
      ".priv",
      ".pub",
      ".pem",
      ".html",
      ".bin",
      ".crt",
      ".key",
      ".tx",
      ".block",
      ".sh",
      ".qry",
      ".list",
      ".sol",
      ".proto",
      ".cfg"
    ],
    "insert_license": false,
    "license_formats": {
      "js|cto|go": {
        "prepend": "/*",
        "append": "*/",
        "eachLine": {
          "prepend": "* "
        }
      },
      "editorconfig|yaml": {
        "prepend": "#",
        "append": "#",
        "eachLine": {
          "prepend": "# "
        }
      }
    }
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://github.com/hyperledger/caliper/blob/master/LICENSE"
    }
  ],
  "description": "Basic CLI driven sample(s)",
  "bugs": {
    "url": "https://github.com/hyperledger/caliper/issues"
  },
  "homepage": "https://github.com/hyperledger/caliper#readme",
  "main": "index.js",
  "keywords": [],
  "author": ""
}
